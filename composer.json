{
  "name": "mygento/module-content",
  "type": "magento2-module",
  "version": "2.4.3",
  "license": "OSL-3.0",
  "homepage": "https://github.com/mygento/module-content",
  "description": "Mygento Content Import Export",
  "authors": [
    {
      "name": "Nikita Tarasov",
      "email": "nikita@mygento.net",
      "homepage": "http://www.mygento.net/",
      "role": "Developer"
    },
    {
      "name": "Mygento",
      "email": "connect@mygento.net",
      "homepage": "https://www.mygento.ru/"
    }
  ],
  "keywords": [
    "magento",
    "mygento"
  ],
  "require": {
    "mygento/module-base": "~2.4.0-RC2",
    "magento/module-cms": "104.*",
    "mustangostang/spyc": "^0.6.2"
  },
  "require-dev": {
      "mygento/coding-standard": "~2.14.0-beta7"
  },
  "minimum-stability": "RC",
  "prefer-stable": true,
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Mygento\\Content\\": ""
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "magento/composer-dependency-version-audit-plugin": true,
      "phpro/grumphp": true
    }
  }
}
